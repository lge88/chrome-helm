{
  "manifest_version": 2,

  "name": "Helm",
  "description": "Incremental completion and selection narrowing framework for google chrome.",
  "version": "1.0",

  "browser_action": {
    "default_title": "helm"
  },

  "background": {
    "scripts": [
      "background.js"
    ]
  },

  "permissions": [
    "tabs",
    "history",
    "bookmarks"
  ],

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+L",
        "windows": "Ctrl+L",
        "mac": "Command+L",
        "chromeos": "Ctrl+L",
        "linux": "Ctrl+L"
      }
    }
  }
}
