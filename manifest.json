{
  "manifest_version": 2,

  "name": "Helm",
  "description": "Incremental completion and selection narrowing framework for google chrome.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "helm"
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks"
  ]
}
